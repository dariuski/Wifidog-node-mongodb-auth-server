<!DOCTYPE html>  
<html lang="en"> 
 <% include head %> 

<body class="login-body">
  <div id="wrapper" >
<div id="content-wrapper-body" > 

<div class="product-section">
<div class="form-login login-section">

  <img  width="150px" src="/image/logo.png"/>
  <p class="quizsubtitle">Welcome to Osperi</p>
  <form class="login-form" action="/login" method="post">
    <input id="email" type="text" name="email" value="" placeholder="Enter email to verify">
     <input id="name" type="text" name="name" placeholder="Name">
    <input type="hidden" name="redirect_url" value="<%= redirect_url %>" />
    <input type="hidden" name="ip" value="<%= ip %>" /> 
    <input type="hidden" name="gwid" value="<%= gwid %>" /> 
    <button id="btn-signup" type="submit" class="btn btn-login"><i class="icon-hand-right"></i>Access internet</button>
  </form>
    
</div>
</div>

</div>


<!-- End footer -->

<script src="/javascripts/jquery.js"></script>  
<script src="/javascripts/bootstrap.min.js"></script> 
<script src="/javascripts/userauth.js"></script> 



</div> 
</body> 

</html>  
